{
  "kind": "collectionType",
  "collectionName": "categories",
  "info": {
    "singularName": "category",
    "pluralName": "categories",
    "displayName": "Category",
    "description": ""
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "unique": false,
      "required": true
    },
    "parent_categories": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::category.category",
      "inversedBy": "child_categories"
    },
    "child_categories": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::category.category",
      "mappedBy": "parent_categories"
    },
    "identifier": {
      "type": "string",
      "required": false,
      "unique": false
    },
    "image": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images"
      ]
    },
    "content": {
      "type": "richtext"
    },
    "summary": {
      "type": "text"
    },
    "slug": {
      "type": "text"
    },
    "original_categories": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::original-category.original-category",
      "mappedBy": "parent_category"
    },
    "level": {
      "type": "integer"
    }
  }
}
