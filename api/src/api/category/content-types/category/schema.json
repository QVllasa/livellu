{
  "kind": "collectionType",
  "collectionName": "categories",
  "info": {
    "singularName": "category",
    "pluralName": "categories",
    "displayName": "Category",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "unique": false,
      "required": true
    },
    "parent_categories": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::category.category",
      "inversedBy": "child_categories"
    },
    "child_categories": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::category.category",
      "mappedBy": "parent_categories"
    },
    "cat1": {
      "type": "text"
    },
    "identifier": {
      "type": "text",
      "required": true,
      "unique": true
    },
    "cat2": {
      "type": "string"
    },
    "cat3": {
      "type": "string"
    },
    "isCategory": {
      "type": "boolean",
      "default": false
    },
    "ignored": {
      "type": "boolean",
      "default": false
    },
    "article_categories": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::article-category.article-category",
      "mappedBy": "category"
    },
    "image": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images"
      ]
    },
    "content": {
      "type": "richtext"
    },
    "navigation_item": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::navigation.navigation",
      "inversedBy": "category"
    },
    "summary": {
      "type": "text"
    }
  }
}
